{
    "name": "davletshindf/telegram-bug-notification",
    "description": "Sending error notifications in telegram",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Davletshin Denis",
            "email": "zloi.genii@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Davletshindf\\TelegramBugNotification\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vlucas/phpdotenv": "^v5.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Davletshindf\\TelegramBugNotification\\Providers\\TelegramServiceProvider"
            ],
            "aliases": {
                "Telegram": "Davletshindf\\TelegramBugNotification\\Facades\\Telegram"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
